<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_scheduler_cancel_expired_quotations" model="ir.cron">
            <field name="name">Sale Quotations: cancel expired quotations</field>
            <field name="model_id">sale.order</field>
            <field name="state">code</field>
            <field name="code">sale.order._cancel_expired_quotations</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now()
            timedelta(days=1)).strftime('%Y-%m-%d 00:00:00')" />
        </record>
    </data>
</odoo>